{
  "name": "restified-endpoints",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "deploy": "wrangler deploy",
    "dev": "wrangler dev",
    "start": "wrangler dev",
    "typecheck": "tsc --noEmit --skipLibCheck",
    "typecheck:watch": "tsc --noEmit --skipLibCheck --watch",
    "lint": "npx --yes prettier --check .",
    "format": "npx --yes prettier --write .",
    "build": "tsc",
    "build:lambda": "npm run build && npm run install-lambda-packages",
    "install-lambda-packages": "cp package.lambda.json dist/package.json && cd dist && npm install",
    "serve": "NODE_PATH=./dist node dist/app.js",
    "serve-dev": "nodemon src/app.ts",
    "test": "jest",
    "test:update": "jest --updateSnapshot"
  },
  "devDependencies": {
    "@types/express": "^5.0.3",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.7.2",
    "aws-cdk": "2.1030.0",
    "jest": "^30.2.0",
    "nodemon": "^3.1.10",
    "ts-jest": "^29.4.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3",
    "wrangler": "^4.43.0"
  },
  "dependencies": {
    "@microlabs/otel-cf-workers": "^1.0.0-rc.52",
    "@opentelemetry/api": "1.9.0",
    "@opentelemetry/core": "2.1.0",
    "@opentelemetry/instrumentation": "0.206.0",
    "@opentelemetry/instrumentation-http": "0.206.0",
    "@opentelemetry/propagator-b3": "2.1.0",
    "@opentelemetry/resources": "2.1.0",
    "@opentelemetry/sdk-trace-base": "2.1.0",
    "@opentelemetry/sdk-trace-node": "2.1.0",
    "@opentelemetry/semantic-conventions": "1.37.0",
    "aws-cdk-lib": "2.220.0",
    "constructs": "^10.0.0",
    "express": "^5.1.0",
    "pino": "^10.0.0",
    "pino-http": "^11.0.0",
    "serverless-http": "^4.0.0",
    "zod": "^4.1.12"
  }
}